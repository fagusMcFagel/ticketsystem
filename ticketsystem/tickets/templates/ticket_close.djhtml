<html>
<head>
    {% load static %}
    <link rel="stylesheet" type="text/css" href={% static 'tickets/stylesheet.css'%}>
    <title>RG - Ticket schließen</title>
</head>
<body>
    <form action="" method="post" novalidate>
    <table>
        <tr>
            <td>
                 <table>
                {{ closeform.as_table }}
                </table>
            </td>
            <td>        
                <table>
                {{ detailform.as_table }}
                </table>
            </td>
        </tr>
    </table>
    {% csrf_token %}
    <input type="submit" value="Ticket schließen">
    </form>
    <a href={% url 'list_ticket' %}><button type="button">Übersicht</button></a>
    <a href={% url 'edit_ticket' detailform.ticketid.value %}><button type="button">Zu Bearbeitung</button></a>
</body>
</html>